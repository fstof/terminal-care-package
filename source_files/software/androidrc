# Android env vars

### change this to your Android SDK install location
export ANDROID_HOME=$HOME/Library/Android/sdk
###

### more vars and PATH
export ANDROID_SDK=$ANDROID_HOME
export ANDROID_SDK_ROOT=$ANDROID_HOME
export ANDROID_BUILD_TOOLS_VERSION=30.0.3
export PATH=$PATH:$ANDROID_HOME/emulator:$ANDROID_HOME/tools:$ANDROID_HOME/tools/bin:$ANDROID_HOME/platform-tools:$ANDROID_HOME/build-tools/$ANDROID_BUILD_TOOLS_VERSION
